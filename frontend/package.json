{
  "name": "ghost-minter-view",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.7",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/webpack": "4.1.4",
    "@walletconnect/web3-provider": "^1.3.1",
    "bignumber.js": "^9.0.1",
    "dotenv": "^10.0.0",
    "ethers": "^5.0.23",
    "html-webpack-plugin": "^4.0.0",
    "material-ui-popup-state": "^1.7.1",
    "notistack": "^1.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^4.1.2",
    "web-vitals": "^0.2.4",
    "web3": "^1.3.6",
    "web3-eth-contract": "^1.3.6",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/webpack serve --open",
    "build": "./node_modules/.bin/webpack build --config webpack.prod.js",
    "heroku-postbuild": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean:contracts": "rm -rf src/contracts && rm -rf src/typechain",
    "clean:modules": "rm -rf node_modules",
    "clean": "npm run clean:contracts && npm run clean:modules"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "exclude": [
    "typings/browser.d.ts",
    "typings/browser",
    "node_modules"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}